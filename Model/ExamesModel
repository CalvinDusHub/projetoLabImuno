public class ExamesSolicitados {

    private String respostaMicrobio;
    private String respostaParasito;
    private String respostaHemato;
    private String respostaUrina;
    private String respostaBioquimica;
    private Boolean jejum;

    public ExamesSolicitados(String nomeExame, String dataSolicitacao, String medicoSolicitante) {
        this.respostaMicrobio = respostaMicrobio;
        this.respostaParasito = respostaParasito;
        this.respostaHemato = respostaHemato;
        this.respostaUrina = respostaUrina;
        this.respostaBioquimica = respostaBioquimica;
        this.jejum = false;
    }

    public String getRespostaMicrobio() {
        return respostaMicrobio;
    }

    public void setRespostaMicrobio(String respostaMicrobio) {
        this.respostaMicrobio = respostaMicrobio;
    }

    public String getRespostaParasito() {
        return respostaParasito;
    }

    public void setRespostaParasito(String respostaParasito) {
        this.respostaParasito = respostaParasito;
    }

    public String getRespostaHemato() {
        return respostaHemato;
    }

    public void setRespostaHemato(String respostaHemato) {
        this.respostaHemato = respostaHemato;
    }

    public String getRespostaUrina() {
        return respostaUrina;
    }

    public void setRespostaUrina(String respostaUrina) {
        this.respostaUrina = respostaUrina;
    }

    public String getRespostaBioquimica() {
        return respostaBioquimica;
    }

    public void setRespostaBioquimica(String respostaBioquimica) {
        this.respostaBioquimica = respostaBioquimica;
    }

    public Boolean getJejum() {
        return jejum;
    }

    public void setJejum(Boolean jejum) {
        this.jejum = jejum;
    }

    @Override
    public String toString() {
        return "ExamesSolicitados{" +
                "respostaMicrobio='" + respostaMicrobio + '\'' +
                ", respostaParasito='" + respostaParasito + '\'' +
                ", respostaHemato='" + respostaHemato + '\'' +
                ", respostaUrina='" + respostaUrina + '\'' +
                ", respostaBioquimica='" + respostaBioquimica + '\'' +
                ", jejum=" + jejum +
                '}';
    }
}
